public class If{
  public static void main (String[] args) {
    int it = 5;
    String str;
    if (it==5) {
      str = "it's the same!";
      }
    else if (it < 5) {
      str = "it's so small!";
      }
    else {
      str = "it's so big...";
      }
      System.out.println(str);
    }
  }
  
  //if (boolean expression) {
      //what happens when true
//      }
//      if false, move on...
//    else if (boolean) {other statement if new boolean is true}
//    else 
//     (if false)
